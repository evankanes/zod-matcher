{
  "name": "zod-matcher",
  "version": "1.0.0",
  "author": "Evan Kanes",
  "description": "Rust inspired matcher for Zod schemas",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
