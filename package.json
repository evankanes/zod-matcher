{
  "name": "zod-matcher",
  "version": "1.0.3",
  "author": "Evan Kanes",
  "description": "Rust inspired pattern matching for Zod schemas",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "license": "MIT",
  "keywords": [
    "matching",
    "match",
    "matcher",
    "pattern-matching",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evankanes/zod-matcher"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest src",
    "format": "yarn prettier --write .",
    "lint": "yarn eslint src",
    "build": "rm -rf dist/ && yarn format && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --project tsconfig.build.json --outDir dist/esm",
    "build:cjs": "tsc --project tsconfig.build.json --outDir dist/cjs --module CommonJS"
  },
  "dependencies": {
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^5.0.3"
  }
}
